<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cblue.messengerserver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".MessengerService01">
            <intent-filter>
                <action android:name="com.cblue.messengerserver.MessengerService01" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>

        <activity
            android:name=".MessengerService_Activity01"
            android:label="@string/title_activity_messenger_service_01"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>